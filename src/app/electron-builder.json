{
  "productName": "Inshapardaz",
  "directories": {
    "app": "dist",
    "output": "../../dist"
  },

  "win": {
    "icon": "dist/favicon",
    "target": [
      "NSIS"
    ],
    "extraResources": {
      "from": "../api/bin/dist/win",
      "to": "api/bin",
      "filter": [
        "**/*"
      ]
    }
  },
  "mac": {
    "target": [
      "dmg"
    ],
    "extraResources": {
      "from": "../api/bin/dist/osx",
      "to": "api/bin",
      "filter": [
        "**/*"
      ]
    }
  },
  "linux": {
    "category": "Utility",
    "target": [
      "AppImage",
      "deb"
    ],
    "extraResources": {
      "from": "../api/bin/dist/linux64",
      "to": "api/bin",
      "filter": [
        "**/*"
      ]
    }
  }
}